{
    "game_name": "Tetris_Battle",
    "version": "3.1",
    "description": "Multiplayer Tetris Battle",
    "server": {
        "script": "game_server.py",
        "args_template": "--port {port} --token {token} --room {room_id} --lobby_host {lobby_host} --lobby_port {lobby_port}"
    },
    "client": {
        "script": "client_gui.py",
